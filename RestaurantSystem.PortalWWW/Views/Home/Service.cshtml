@model IEnumerable<RestaurantSystem.Data.Data.CMS.ServicePartial>;

@if (Model is not null && Model.Any())
{
    <!-- Service Start -->
    @foreach (var partial in Model)
    {

        <div class="col-lg-4 col-md-6">
            <div class="bg-primary border-inner text-center text-white p-5">
                <h4 class="text-uppercase mb-3">@partial.Title</h4>
                <p>@partial.SubTitle</p>
            </div>
        </div>
    }
    <!-- Service End -->
}